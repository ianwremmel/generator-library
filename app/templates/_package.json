{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {<% if (bowerIncludes) { %>
    "postinstall": "npm/postinstall.sh", <% } %>
    "test": "./node_modules/.bin/grunt test"
  },
  "devDependencies": {<% if (supportBrowsers) { %>
    "grunt-shell": "^0.6.4",
    "browserify": "^3.32.1",<% } if (bowerIncludes) { %>
    "bower": "^1.3.1",
    "debowerify": "^0.7.0",<% } %>
    "grunt-cli": "^0.1.13",
    "grunt": "^0.4.0",
    "time-grunt": "^0.2.10",
    "load-grunt-tasks": "^0.2.0",
    "grunt-bump": "^0.0.3",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-jshint": "^0.9.2",
    "jshint-stylish": "^0.1.3",
    "grunt-jscs-checker": "0.4.0"
  },<% if (supportBrowsers) { %>
  "browserify": {
    "transform": [<% if (bowerIncludes) { %>
      "debowerify",<% } %>
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "underscore": "global:_"
  },<% } %>
  "private": true
}
